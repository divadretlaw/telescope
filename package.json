{
    "name": "telescope",
    "version": "0.0.1",
    "description": "Calculate metrics on RAW Astro-Images",
    "homepage": "https://github.com/divadretlaw/telescope",
    "author": {
        "name": "David Walter"
    },
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:electron": "ng build --configuration=electron --base-href ./",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron:local": "npm run build:electron && electron .",
        "electron:linux": "npm run build:electron && electron-builder build --linux",
        "electron:windows": "npm run build:electron && electron-builder build --windows",
        "electron:mac": "npm run build:electron && electron-builder build --mac"
    },
    "build": {
        "appId": "at.davidwalter.telescope",
        "productName": "Telescope",
        "copyright": "Copyright Â© 2021 ${author}",
        "asar": true,
        "asarUnpack": [
            "astrograph/rocket.py", "astrograph/brightness_curve.py"
        ],
        "directories": {
            "output": "release"
        },
        "files": ["main.js", "dist/**", "astrograph/*"],
        "mac": {
            "icon": "icon.png"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.2.0",
        "@angular/common": "~11.2.0",
        "@angular/compiler": "~11.2.0",
        "@angular/core": "~11.2.0",
        "@angular/forms": "~11.2.0",
        "@angular/platform-browser": "~11.2.0",
        "@angular/platform-browser-dynamic": "~11.2.0",
        "@angular/router": "~11.2.0",
        "electron-is-packaged": "^1.0.2",
        "primeicons": "^4.1.0",
        "primeng": "^11.2.3",
        "python-shell": "^2.0.3",
        "reset-css": "^5.0.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.0",
        "@angular/cli": "~11.2.0",
        "@angular/compiler-cli": "~11.2.0",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.20.1",
        "codelyzer": "^6.0.0",
        "electron": "^11.2.3",
        "electron-builder": "^22.9.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.1.2"
    }
}
