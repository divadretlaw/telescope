{
    "name": "telescope",
    "version": "0.0.1",
    "description": "Calculate metrics on RAW Astro-Images",
    "homepage": "https://github.com/divadretlaw/telescope",
    "author": {
        "name": "David Walter"
    },
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:electron": "ng build --configuration=electron --base-href ./",
        "build:rocket": "pyinstaller astrograph/rocket.py --noconfirm --distpath './resources'",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "rocket:launch": "export FLASK_APP=astrograph/rocket.py && flask run --port 40270",
        "electron:local": "npm run build:electron && npm run build:rocket && electron .",
        "electron:linux": "npm run build:electron && npm run build:rocket && electron-builder build --linux",
        "electron:windows": "npm run build:electron && npm run build:rocket && electron-builder build --windows",
        "electron:mac": "npm run build:electron && npm run build:rocket && electron-builder build --mac"
    },
    "build": {
        "appId": "at.davidwalter.telescope",
        "productName": "Telescope",
        "copyright": "Copyright Â© 2021 ${author}",
        "asar": true,
        "directories": {
            "output": "release"
        },
        "files": [
            "main.js",
            "dist/**"
        ],
        "extraResources": [
            "resources/**"
        ],
        "mac": {
            "icon": "src/assets/icon.png"
        }
    },
    "private": true,
    "dependencies": {
        "electron-is-packaged": "^1.0.2",
        "primeicons": "^4.1.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1102.1",
        "@angular/animations": "^11.2.2",
        "@angular/cli": "^11.2.1",
        "@angular/common": "^11.2.2",
        "@angular/compiler": "^11.2.2",
        "@angular/compiler-cli": "^11.2.2",
        "@angular/core": "^11.2.2",
        "@angular/forms": "^11.2.2",
        "@angular/platform-browser": "^11.2.2",
        "@angular/platform-browser-dynamic": "^11.2.2",
        "@angular/router": "^11.2.2",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^14.14.31",
        "electron": "^11.2.3",
        "electron-builder": "^22.9.1",
        "eslint": "^5.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ngx-device-detector": "^2.0.5",
        "primeng": "^11.2.3",
        "protractor": "~7.0.0",
        "reset-css": "^5.0.1",
        "rxjs": "~6.6.3",
        "ts-node": "~9.1.1",
        "tslib": "^2.1.0",
        "typescript": "~4.1.5",
        "zone.js": "^0.10.2"
    }
}
